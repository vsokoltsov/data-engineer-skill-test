{
    "uid": "ingest-logs",
    "title": "Ingest Logs (Loki)",
    "tags": ["logs", "loki", "ingest"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "5s",
    "time": { "from": "now-30m", "to": "now" },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
      "time_options": ["5m", "15m", "30m", "1h", "3h", "6h", "12h", "24h"]
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "label": "service",
          "type": "query",
          "datasource": { "type": "loki", "uid": "loki" },
          "refresh": 2,
          "hide": 0,
          "includeAll": true,
          "allValue": ".+",
          "multi": true,
          "sort": 1,
          "query": "label_values({job=~\".+\"}, service)",
          "current": { "selected": true, "text": "All", "value": "$__all" }
        },
        {
          "name": "source",
          "label": "source",
          "type": "query",
          "datasource": { "type": "loki", "uid": "loki" },
          "refresh": 2,
          "hide": 0,
          "includeAll": true,
          "allValue": ".+",
          "multi": true,
          "sort": 1,
          "query": "label_values({service=~\"$service\"}, source)",
          "current": { "selected": true, "text": "All", "value": "$__all" }
        },
        {
          "name": "level",
          "label": "level",
          "type": "query",
          "datasource": { "type": "loki", "uid": "loki" },
          "refresh": 2,
          "hide": 0,
          "includeAll": true,
          "allValue": ".+",
          "multi": true,
          "sort": 1,
          "query": "label_values({service=~\"$service\"}, level)",
          "current": { "selected": true, "text": "All", "value": "$__all" }
        },
        {
          "name": "search",
          "label": "search",
          "type": "textbox",
          "hide": 0,
          "current": { "text": "", "value": "" }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "type": "timeseries",
        "title": "Log rate (lines/sec)",
        "gridPos": { "x": 0, "y": 0, "w": 24, "h": 7 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [
          {
            "refId": "A",
            "queryType": "range",
            "expr": "sum by (service) (rate({service=~\"$service\", source=~\"$source\", level=~\"$level\"}[1m]))"
          }
        ],
        "options": {
          "legend": { "showLegend": true },
          "tooltip": { "mode": "single" }
        }
      },
      {
        "id": 2,
        "type": "logs",
        "title": "Logs",
        "gridPos": { "x": 0, "y": 7, "w": 24, "h": 14 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [
          {
            "refId": "A",
            "queryType": "range",
            "expr": "{service=~\"$service\", source=~\"$source\", level=~\"$level\"} |= \"$search\""
          }
        ],
        "options": {
          "dedupStrategy": "none",
          "enableLogDetails": true,
          "prettifyLogMessage": true,
          "showCommonLabels": true,
          "showLabels": true,
          "showTime": true,
          "wrapLogMessage": true
        }
      }
    ],
    "annotations": { "list": [] },
    "links": []
  }